{
  "name": "onestat",
  "version": "1.2.0",
  "private": true,
  "author": "Ihor Mazurenko",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www & npm run scss",
    "scss": "sass --watch src/components/base/base.scss public/stylesheets/main.css",
    "seed:enums": "node scripts/seedEnums.js",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:prebeta": "standard-version --prerelease beta",
    "release:prerc": "standard-version --prerelease rc",
    "postrelease": "git push --follow-tags origin $(git branch --show-current)",
    "version": "echo 'Use npm run release instead of npm version' && exit 1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "connect-mongodb-session": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "csv": "^6.3.9",
    "csv-writer": "^1.6.0",
    "debug": "~2.6.9",
    "express": "^4.21.0",
    "express-recaptcha": "^5.1.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "http-errors": "~1.6.3",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^15.10.0",
    "gulp": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "2.0.0",
    "nodemon": "^3.1.7",
    "sass": "^1.80.4",
    "standard-version": "^9.5.0"
  }
}
